<form method="post" enctype="multipart/form-data">
    <div class="container">
        <div class="row">
              <div class="input-group my-2">
                 <input name="Name" id="name" class="form-control" type="text" placeholder="Name"/>
              </div>
               <div class="input-group my-2">
                 <input name="FeedBack" id="feedBack" class="form-control" type="text" placeholder="FeedBack"/>
              </div>
               <div class="input-group my-2">
                 <input name="myPhoto" id="photo" class="form-control" type="file" placeholder="Customer Photo"/>
              </div>
              <input type="submit" class="btn btn-success"value="Create"/>

        </div>
    </div>
</form>